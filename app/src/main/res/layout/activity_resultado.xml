<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="resultadoVM" type="com.paipbook.app.modules.resultado.data.viewmodel.ResultadoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameResultado"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <View android:background="@color/light_green_900_28"
            android:id="@+id/viewRectangleThirtyTwo"
            android:layout_gravity="start"
            android:layout_height="@dimen/_753pxh" android:layout_width="@dimen/_360pxh"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_40pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <ImageView android:id="@+id/imageHome"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_122pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home_27x33"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home_27x33"/>
                    <ImageView android:id="@+id/imageTrash"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_123pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_trash"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_trash"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageLivro"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_256pxh"
                android:layout_marginEnd="@dimen/_59pxh"
                android:layout_marginStart="@dimen/_59pxh"
                android:layout_marginTop="@dimen/_68pxv"
                android:layout_width="@dimen/_168pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_livro}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_livro"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.03"
                android:text="@{resultadoVM.resultadoModel.txtDescription}"
                style="@style/txtRobotoblack13_1" tools:text="@string/msg_nome_uma_avent"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnAdicionarReserva"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_53pxh"
                android:layout_marginStart="@dimen/_59pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.03"
                android:paddingBottom="@dimen/_23pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_23pxv"
                android:singleLine="true"
                android:text="@string/msg_adicionar_res"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline_2" tools:text="@string/msg_adicionar_res"/>
        </LinearLayout>
    </FrameLayout>
</layout>
