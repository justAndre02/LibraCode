<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameLivrosrecomend"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_1">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_29pxv"
            android:layout_marginEnd="@dimen/_6pxh"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnArrowleft"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:singleLine="true"
                        android:textAllCaps="false" style="@style/btn"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnHome"
                        android:layout_height="@dimen/_27pxv"
                        android:layout_marginStart="@dimen/_123pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:singleLine="true"
                        android:textAllCaps="false" style="@style/btn"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end" android:id="@+id/txtPDM"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="wrap_content"
                android:singleLine="true" android:text="@string/lbl_pdm"
                style="@style/txtRobotoromanregular40" tools:text="@string/lbl_pdm"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTopThree"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_top_3"
                style="@style/txtRobotoromanblack36" tools:text="@string/lbl_top_3"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowdescription"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_green_900_44">
                <ImageView android:id="@+id/imageImageNine"
                    android:layout_height="@dimen/_139pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_95pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image9}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image9"/>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.71"
                    android:layout_width="0dp"
                    android:letterSpacing="0.03"
                    android:text="@string/msg_nome_android"
                    style="@style/txtRobotoblack13_2" tools:text="@string/msg_nome_android"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowdescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_green_900_44">
                <ImageView android:id="@+id/imageImageTen"
                    android:layout_height="@dimen/_139pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_95pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image10}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image10"/>
                <TextView android:id="@+id/txtDescriptionOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.69"
                    android:layout_width="0dp"
                    android:letterSpacing="0.03"
                    android:text="@string/msg_nome_kotlin_in"
                    style="@style/txtRobotoblack13_2" tools:text="@string/msg_nome_kotlin_in"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowdescriptionTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_green_900_44">
                <ImageView android:id="@+id/imageImageEleven"
                    android:layout_height="@dimen/_118pxh"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="@dimen/_95pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image11}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image11"/>
                <TextView android:id="@+id/txtDescriptionTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.69"
                    android:layout_width="0dp"
                    android:letterSpacing="0.03"
                    android:text="@string/msg_nome_head_firs"
                    style="@style/txtRobotoblack13_2" tools:text="@string/msg_nome_head_firs"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
