<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameRequisio"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_1">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnArrowleft"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:singleLine="true"
                        android:textAllCaps="false" style="@style/btn"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnHome"
                        android:layout_height="@dimen/_27pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_129pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:singleLine="true"
                        android:textAllCaps="false" style="@style/btn"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/framePesquisa"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_49pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="@dimen/_318pxh" style="@style/groupStylebluegray_100_99">
                <LinearLayout android:id="@+id/linearRowumaaventura"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUmaAventura"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_uma_aventura"
                        style="@style/txtRobotoromanregular15" tools:text="@string/lbl_uma_aventura"/>
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_192pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_black_900"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/framePesquisa1"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_49pxv"
                    android:layout_width="@dimen/_318pxh" style="@style/groupStylebluegray_100_99">
                    <ImageView android:id="@+id/imageSearchOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_black_900"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndescription"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_1">
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_97pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.03"
                    android:text="@string/msg_nome_don_quixo"
                    style="@style/txtRobotoblack13_2" tools:text="@string/msg_nome_don_quixo"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
