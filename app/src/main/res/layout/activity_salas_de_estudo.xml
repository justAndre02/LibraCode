<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearSalasdeEstudo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <ImageView
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnHome"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_27pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="@dimen/_33pxh"
            android:singleLine="true" android:textAllCaps="false" style="@style/btn"
            android:src="@drawable/image_home"/>
        <TextView android:ellipsize="end" android:id="@+id/txtLogin"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@string/lbl_salas_de_estudo"
            style="@style/txtRobotoromanregular36" tools:text="@string/lbl_salas_de_estudo"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearSala1"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelight_green_200">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtPrimeiraSala"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_sala_1_16_luga"
                style="@style/txtRobotoromanblack36_1" tools:text="@string/msg_sala_1_16_luga"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearSala2"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelight_green_200">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtSegundaSala"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_sala_2_4_lugar"
                style="@style/txtRobotoromanblack36_1" tools:text="@string/msg_sala_2_4_lugar"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearSala3"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelight_green_200">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtTerceiraSala"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_sala_3_4_lugar"
                style="@style/txtRobotoromanblack36_1" tools:text="@string/msg_sala_3_4_lugar"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearSala4"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylelight_green_200">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtQuartaSala"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_sala_4_4_lugar"
                style="@style/txtRobotoromanblack36_1" tools:text="@string/msg_sala_4_4_lugar"/>
        </LinearLayout>
    </LinearLayout>
</layout>
